{
  "manifest_version": 2,
  "name": "Test account switcher",
  "version": "1.0",
  "permissions": ["cookies", "<all_urls>", "tabs", "activeTab"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "web_accessible_resources": [
    "accounts.json"
  ]
}
